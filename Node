public class NodeDoublyLinkedList {
    private int data;
    NodeDoublyLinkedList next;
    NodeDoublyLinkedList prev;

    public NodeDoublyLinkedList() {
        next = null;
        prev = null;
    }

    public NodeDoublyLinkedList(int data) {
        this.data = data;
        next = null;
        prev = null;
    }

    public void setData(int data) {
        this.data = data;
    }

    public int getData() {
        return data;
    }
}
